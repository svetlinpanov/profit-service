{
  "name": "profit-sservice",
  "version": "0.0.1",
  "description": " \"Profit Service\"",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "yarn build && node ./dist/service.js",
    "start:dev": "nodemon ./src/service.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "tsc --noEmit && eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "NODE_ENV=test PORT=3001 jest --detectOpenHandles --runInBand",
    "test:coverage": "jest --collect-coverage --coverageDirectory coverage --runInBand",
    "test:dev": "yarn test --watchAll --collect-coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svetlinpanov/profit-service.git"
  },
  "author": "Svetin Panov",
  "license": "MIT",
  "homepage": "https://github.com/svetlinpanov/profit-service#readme",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "dotenv": "^10.0.0",
    "inversify": "^5.1.1",
    "inversify-inject-decorators": "^3.1.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.8.3",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "winston": "^3.3.3",
    "zod": "^3.20.2",
    "zod-validation-error": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.3",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.3.0",
    "@types/node": "^16.4.13",
    "@types/node-fetch": "^2.5.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.11.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9"
  }
}